{
    "name": "LSTM",
    "data_loader": {
        "name": "Android-Universal-Image-Loader",
        "full_data": "all_methoddeclarations_train",
        "local_data": "bigbluebutton_methoddeclarations_train",
        "window_size": 8,
        "max_words": 40000
    },
    "model": {
        "name": "model.LSTM",
        "embedding_dim": 256,
        "lstm_dim": 128,
        "dense_dim": 100,
        "dropout_1": 0.5,
        "dropout_2": 0.5,
        "loss": "sparse_categorical_crossentropy",
        "metrics": [
            "acc"
        ]
    },
    "trainer": {
        "name": "trainer.LSTM",
        "num_epochs": 15,
        "batch_size": 128,
        "validation_split": 0.2
    }
}