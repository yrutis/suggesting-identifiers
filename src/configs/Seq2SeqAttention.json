{
    "name": "seq2seqAttention",
    "data_loader": {
        "name": "java-small-project-split",
        "window_size_body": 8,
        "window_size_params": 2,
        "window_size_name": 5
    },
    "model": {
        "name": "model.seq2seq",
        "embedding_dim": 64,
        "gru_dim": 50,
        "dense_dim": 50,
        "dropout_1": 0.5,
        "loss": "categorical_crossentropy",
        "metrics": [
            "acc"
        ]
    },
    "trainer": {
        "name": "trainer.seq2seq",
        "num_epochs": 2,
        "batch_size": 64
    }
}