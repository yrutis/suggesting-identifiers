{
    "name": "seq2seqAttention",
    "mode": "eval",
    "data_loader": {
        "name": "Android-Universal-Image-Loader",
        "window_size_body": 8,
        "window_size_params": 2,
        "window_size_name": 5,
        "partition": 32
    },
    "model": {
        "name": "model.seq2seq",
        "embedding_dim": 256,
        "gru_dim": 250,
        "recurrent_dropout_1": 0.2,
        "dropout_1": 0.2,
        "recurrent_dropout_2": 0.2,
        "dropout_2": 0.2,
        "loss": "categorical_crossentropy",
        "metrics": [
            "acc"
        ]
    },
    "trainer": {
        "name": "trainer.seq2seq",
        "num_epochs": 2,
        "batch_size": 8
    }
}