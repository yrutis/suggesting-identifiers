{
    "name": "LSTMBid",
    "data_loader": {
        "name": "Android-Universal-Image-Loader",
        "full_data": "all_methods_train",
        "local_data": "Android-Universal-Image-Loader",
        "window_size_body": 16,
        "window_size_params": 8,
        "remove_train_unk": 0.5,
        "remove_val_unk": 0.5
    },
    "model": {
        "name": "model.LSTMBid",
        "embedding_dim": 64,
        "lstm_dim": 32,
        "dense_activation_1": "sigmoid",
        "dense_dim": 50,
        "dropout_1": 0.5,
        "loss": "sparse_categorical_crossentropy",
        "metrics": [
            "acc"
        ]
    },
    "trainer": {
        "name": "trainer.LSTMBid",
        "num_epochs": 2,
        "batch_size": 64
    }
}