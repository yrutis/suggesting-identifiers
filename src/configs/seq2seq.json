{
    "name": "seq2seq",
    "data_loader": {
        "name": "Android-Universal-Image-Loader",
        "window_size_body": 25,
        "window_size_params": 4,
        "window_size_name": 5
    },
    "model": {
        "name": "model.seq2seq",
        "embedding_dim": 256,
        "lstm_encoder_dim": 250,
        "lstm_decoder_dim": 250,
        "recurrent_dropout_1": 0.2,
        "dropout_1": 0.2,
        "recurrent_dropout_2": 0.2,
        "dropout_2": 0.2,
        "loss": "categorical_crossentropy",
        "metrics": [
            "acc"
        ]
    },
    "trainer": {
        "name": "trainer.seq2seq",
        "num_epochs": 2,
        "batch_size": 64
    }
}