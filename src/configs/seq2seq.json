{
    "name": "seq2seq",
    "data_loader": {
        "name": "Android-Universal-Image-Loader",
        "window_size_body": 25,
        "window_size_params": 4,
        "window_size_name": 5,
        "remove_train_unk": 0.5,
        "remove_val_unk": 0.5
    },
    "model": {
        "name": "model.seq2seq",
        "embedding_dim": 64,
        "lstm_encoder_dim": 50,
        "lstm_decoder_dim": 50,
        "dense_dim": 50,
        "dropout_1": 0.5,
        "loss": "categorical_crossentropy",
        "metrics": [
            "acc"
        ]
    },
    "trainer": {
        "name": "trainer.seq2seq",
        "num_epochs": 2,
        "batch_size": 64
    }
}